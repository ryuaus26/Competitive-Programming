#include <iostream>
#include <string>
#include <algorithm>
#include <vector>
#include <map>
#include <unordered_map>
#include <cmath>

using namespace std;

#define ll long long



int main(){
    string a;
    cin >> a;
    sort(a.begin(),a.end());
    vector<string>ans;
    do{
        ans.push_back(a);
    }while(next_permutation(a.begin(),a.end())); //MAKE SURE YOU USE DO LOOP ELSE YOUR ANSWER WILL BE 1 OFF!!!
    cout << ans.size() << endl;
    for(string a: ans){
        cout << a << endl;
    }
    

}
