#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define vi vector<int>
#define pb push_back




void solve(){
  int n,k; cin >> n>>k;
  int count = 0;
  for(int i =0 ;i <= n;i++){
    string a; cin >> a;
    count += a.length();
    if(count <= k){
     cout << " ";
      cout << a;
    }else{
      cout << endl << a;
      count = a.length();
    }
    
  }

}
 
int main()
{
  solve();
  

 
 }

	
