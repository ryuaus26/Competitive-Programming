#include <bits/stdc++.h>
using namespace std;




void solve(){
  int m,n,k;
  cin >> m >> n >> k;
  for(int i =0 ;i < m;i++){
    string a;
    cin >> a;
    for(int it = 0; it < k;it++){
      for(int j = 0;j < n;j++){
        for(int K = 0; K < k;K++){
          cout << a[j];
        }
      }
    cout << endl;
    }
  }

} 
 
int main()
{
  solve();


	
}
