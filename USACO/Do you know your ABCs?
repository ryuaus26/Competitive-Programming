#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define vi vector<int>
#define pb push_back




void solve(){
  vector<int>v(7);
  for(int i = 0;i  < 7;i++){
    cin >> v[i];
  }
  sort(v.begin(),v.end());
  int a = v[0];
  int b = v[1];
  int c = v[6] - v[0] - v[1];

  cout << a << " " << b << " " << c << endl;
}

int main()
{
  solve();
  

 
 }

	
