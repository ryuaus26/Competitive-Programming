#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define vi vector<int>
#define f first
#define s second



void solve(){
	int n,x,y;
	cin >> n >> x >> y;
	if( y == 0){
		cout << "YES" << endl;
		return;
	}

	for(int i = 1;i <= n;i++){
		if(x * i == y){
			cout << "YES" << endl;
			return;
		}
	}
	cout << "NO" << endl;
	
}

int main(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin >> t;
	while(t--){
		solve();
	}
	

}
