#include <iostream>
using namespace std;

void solve() {
	// your code goes here

	    float a,b,x,y;
	cin >> a >> b >> x >> y;
	
	if(a/x  > b/y){
	    cout << "Chefina" << endl;
	}else if(b/y > a/x){
	    cout << "Chef" << endl;
	}else{
	    cout << "Both" << endl;
	}

	

}

int main(){
    int t;
    cin >> t;
    while(t--){
        solve();
    }
}
