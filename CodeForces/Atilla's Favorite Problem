#include<bits/stdc++.h>
using namespace std;

#define ar array
#define ll long long
#define ld long double
#define sza(x) ((int)x.size())
#define all(a) (a).begin(), (a).end()

#define PI 3.1415926535897932384626433832795l
const int MAX_N = 1e5 + 5;
const ll MOD = 1e9 + 7;
const ll INF = 1e9;
const ld EPS = 1e-9;

void solve(){
  string s;
  int n;
  cin >> n;
  cin >> s;
  int count = 0;
  sort(s.begin(),s.end());
    for(char a = 'a';a <= 'z';a++){
      count++;
      if(s.back() == a){
        break;
      }
    
  }

  cout << count << "\n";

}


int main(){
  ios_base::sync_with_stdio(0) ; cin.tie(0) ; cout.tie(0);
int t;
cin >> t;
while(t--)
  solve();
}
