#define all(a) (a).begin(), (a).end()

#define PI 3.1415926535897932384626433832795l
const int MAX_N = 1e5 + 5;
const ll MOD = 1e9 + 7;
const ll INF = 1e9;
const ld EPS = 1e-9;

void solve(){
  int nums[3];
  cin >> nums[0] >> nums[1] >> nums[2];
  sort(nums,nums + 3);
cout << nums[1] << "\n";

}


int main(){
